# itch.io Configuration
# Copy this file to .env and fill in your actual values

# Your itch.io username
ITCH_USER=your-username

# Your itch.io game/project name (as it appears in the URL)
ITCH_GAME=your-game-name

# Your butler API key from https://itch.io/user/settings/api-keys
# The key should have "butler" as its source
BUTLER_API_KEY=your-api-key-here

# Channel name for uploads (e.g., construct3-stable, construct3-beta)
ITCH_CHANNEL=construct3-stable